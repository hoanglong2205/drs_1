<% provide :title, t("division.title") %>
<h1><%= t "division.edit" %></h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @division] do |f| %>
      <%= render "shared/error_messages", object: @division %>
      <%= f.label :division %>
      <%= f.text_field :descrition, class: "form-control" %>      
      <%= f.submit t("edit.save"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
