<% provide :title, t("position.title") %>
<h1><%= t "position.edit" %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @position] do |f| %>
      <%= render "shared/error_messages", object: @position %>

      <%= f.label :Position %>
      <%= f.text_field :position, class: "form-control" %>
      
      <%= f.submit t("edit.save"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
